{
  "name": "WalkME Test",
  "description": "WalkME Test",
  "version": "0.7",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "WalkME Test"
  },
  "manifest_version": 2
}

